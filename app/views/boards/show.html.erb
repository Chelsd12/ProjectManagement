<h1><%= @board.name %> Lists</h1>
<ul>
    <% @lists.each do |list| %>
        <li>
            <%= link_to list.name, board_list_path(list) %>
        </li>
    <% end %>
</ul>
<%= link_to 'Delete this Board', @board, method: :delete %>
<br />
<%= link_to "Add a List to this Board", new_board_list_path(@board) %>